<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="rabbit.tracking.trackers" name="Tracker Extensions" schema="schema/trackers.exsd"/>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="rabbit.tracking.internal.startup.Startup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <serviceFactory
            factoryClass="rabbit.tracking.internal.UserMonitorFactory">
         <service
               serviceClass="rabbit.tracking.IUserMonitor"></service>
      </serviceFactory>
   </extension>

</plugin>
